<h1>Listing sys_users</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Email</th>
    <th>Role</th>
    <th>Allow access</th>
    <th>Active</th>
    <th>Name</th>
    <th>Sex</th>
    <th>Mobile</th>
    <th>Phone</th>
    <th>Qq</th>
    <th>Weixin</th>
    <th>Weibo</th>
    <th>Blog</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sys_users.each do |sys_user| %>
  <tr>
    <td><%= sys_user.id %></td>
    <td><%= sys_user.email %></td>
    <td><%= sys_user.role %></td>
    <td><%= sys_user.allow_access %></td>
    <td><%= sys_user.active %></td>
    <td><%= sys_user.name %></td>
    <td><%= sys_user.sex %></td>
    <td><%= sys_user.mobile %></td>
    <td><%= sys_user.phone %></td>
    <td><%= sys_user.qq %></td>
    <td><%= sys_user.weixin %></td>
    <td><%= sys_user.weibo %></td>
    <td><%= sys_user.blog %></td>
    <td><%= link_to 'Show', sys_user %></td>
    <td><%= link_to 'Edit', edit_sys_user_path(sys_user) %></td>
    <td><%= link_to 'Destroy', sys_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

